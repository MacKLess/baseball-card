<p id="notice"><%= notice %></p>
<div class="container">
  <center>
    <h1>Others</h1>

    <table>
      <thead>
        <tr>
          <th>End project</th>
          <th>Birthday</th>
          <th>Pulse survey</th>
          <th>Other comments</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @others.each do |other| %>
          <tr>
            <td><%= other.end_project %></td>
            <td><%= other.birthday %></td>
            <td><%= other.pulse_survey %></td>
            <td><%= other.occasion %></td>
            <td><%= link_to 'Show', other %></td>
            <td><%= link_to 'Edit', edit_other_path(other) %></td>
            <td><%= link_to 'Destroy', other, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Other :name, :position, :start_date, :life_quote, :quote, :motto, :strength, :myers_briggs, :birkman, :vision, :short_term_goal, :long_term_goal,  :objective_setting, :personal_goal, :mission, :expectation, :awesome, :need, :receive_feedback, :development, :personal_goal, :personal_goal_two, :personal_goal_three, :country, :state, :city, :willing_to_travel, :areas_to_develop, :remote', new_other_path %>
  </center>
</div>
